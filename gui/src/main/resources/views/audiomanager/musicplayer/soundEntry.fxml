<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" styleClass="soundEntry" stylesheets="@soundEntry.css" type="VBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <AnchorPane VBox.vgrow="NEVER">
      <VBox.margin>
         <Insets top="8.0" />
      </VBox.margin>
      <children>
         <HBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
               <Button fx:id="playPause" mnemonicParsing="false">
                  <font>
                     <Font size="10.0" />
                  </font>
               </Button>
                <Label fx:id="soundTitle" HBox.hgrow="ALWAYS" />
            </children>
         </HBox>
         <Button mnemonicParsing="false" onAction="#remove" styleClass="removeSound" text="X" AnchorPane.rightAnchor="0.0" />
      </children>
   </AnchorPane>
    <ProgressBar fx:id="soundProgress" focusTraversable="false" maxHeight="2.0" maxWidth="1.7976931348623157E308" minHeight="2.0" mouseTransparent="true" prefHeight="2.0" progress="0.0" VBox.vgrow="NEVER" />
</fx:root>
